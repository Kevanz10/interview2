<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title text-center">Publisher House Profile</h3>
  </div>
  <div class="panel-body">
    <div class="row">
      <div class=" text-center"> 
        <table class="table ">
          <tbody>
            <tr>
              <td>Name: <%=@house.name.capitalize%></td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <div class="panel-footer text-center">
    <%= link_to 'Edit Publisher House', edit_publisher_house_path(@house), class: 'btn btn-default'%>
    <%= link_to 'Delete', publisher_house_path, class:'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %>
    <%= link_to 'Cancel', publisher_houses_path, class:'btn btn-default'%>
  </div>
</div>
